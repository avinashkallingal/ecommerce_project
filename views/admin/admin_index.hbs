  <script>
    const ctx = document.getElementById('myChart');
    let xcord = [];
    let ycord = [];
    chartCall();
    async function chartCall() {
      await getData();
      new Chart(ctx, {
        type: 'line',
        data: {
          labels: xcord ,
          datasets: [{
            label: 'Daily Orders',
            data: ycord ,
            borderWidth: 1
          }]
        },
        options: {
          scales: {
            y: {
              beginAtZero: true
            }
          }
        }
      });
    }

   async function getData() {
    try {
        const response = await fetch('/admin/graphData/daily');
        if (!response.ok) {
            throw new Error('Failed to fetch data');
        }
        const data = await response.json();
        console.log(data + " data received on HBS");
        // Iterate over the result array and extract values into separate arrays
          data.forEach(entry => {
            ycord.push(entry.productCount);
            xcord.push(entry.orderDate);
          });
    } catch (error) {
        console.error('Error fetching data:', error);
    }
}
