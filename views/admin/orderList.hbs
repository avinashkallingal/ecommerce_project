    <div class="page-content">
      <!-- Page Header-->
      <div class="page-header no-margin-bottom">
        <div class="container-fluid">
          <h2 class="h5 no-margin-bottom">Products</h2>
        </div>
      </div>
      <!-- Breadcrumb-->
      <div class="container-fluid">
        <ul class="breadcrumb">
          <li class="breadcrumb-item"><a href="/admin/home">Home</a></li>
          <li class="breadcrumb-item active">Products </li>
        </ul>
      </div>
      <section class="no-padding-top">
        <div class="container-fluid">
          <div class="row">

            <div class="col-lg-12">
              <div class="block">
                <div class="title"><strong>Products details</strong></div>
                <!-- button add products -->


                <!-- button ends -->

                <div class="table-responsive">

                  <!-- copy starts -->
                  <table class="table table-striped table-hover text-center">
                    <thead>
                      <tr>
                        <th scope="col">Order Id</th>
                        <th scope="col">Username</th>
                        <th scope="col">Date</th>
                        <th scope="col">Product</th>
                        <th scope="col">Quantity</th>
                        <th scope="col">Total Price</th>
                        <th scope="col">Payment</th>
                        <th scope="col">Delivey Status</th>
                        <th scope="col">Update</th>
                        {{!-- <th scope="col">Details</th> --}}
                      </tr>
                    </thead>
                    {{#each order}}
                    <tbody>
                      <tr>
                        <th>{{this.orderId}}</th>
                        <th>{{this.username}}</th>
                        <td>{{this.orderDate}}</td>
                        <td>{{this.product}}</td>
                        <td>{{this.quantity}}</td>
                        <td>{{subTotal this.price this.quantity}}</td>
                        <td>{{this.payment}}</td>

                        <td>
                          {{#if this.adminCancel}}
                          <button class="btn btn-light" disabled>Canceled</button>
                          {{else}}
                          <a href="/admin/statusUpdate?id={{this.orderId}}&status={{this.status.[0]}}&product={{this.product}}"
                            onclick="return confirm('Are you sure you want to change Order status')"
                            class="btn btn-success">{{this.status.[0]}}</a>
                          {{/if}}

                          {{!-- <button onclick="return confirm('Are you sure to want to change Order status')"
                            class="btn btn-success">{{this.status.[0]}}</button> --}}

                        </td>
                        <td>
                          {{!-- {{#if 'this.status== 'Delivered Successfully'}} --}}
                          {{#if (cancelCondition this.adminCancel this.status.[0])}}
                          <button class="btn btn-light" disabled></button>
                          {{else}}
                          <a href="/admin/cancelOrder?product={{this.product}}&id={{this.orderId}}&username={{this.username}}">
                          
                           
                            <button onclick="return confirm('are you sure to cancel the order')" class="btn btn-danger">Cancel</button>
                           {{/if}}
                          </a>
                        </td>

                       
                        {{!-- <td>
                        <a href="/{{this.productname}}">

                            <button class="btn btn-success">Details</button>
                          </a>
                        </td> --}}
                       
                      </tr>
                    </tbody>
                    {{/each}}
                  </table>
                  <!-- copy ends -->


                </div>
              </div>
            </div>








          </div>
